PORT=4000
API_BEARER_TOKEN=dev-token

# Agent workspace and registry persistence
CLAWOS_AGENTS_DIR=
CLAWOS_AGENT_REGISTRY_PATH=

# Durable data paths (optional but recommended)
CLAWOS_MEMORY_BACKEND=json
CLAWOS_EMBEDDING_PROVIDER=local
CLAWOS_MEMORY_DIR=
CLAWOS_MEMORY_PATH=
CLAWOS_MEMORY_LANCEDB_DIR=
CLAWOS_KNOWLEDGE_DIR=
CLAWOS_KNOWLEDGE_PATH=
CLAWOS_COSTS_DIR=
CLAWOS_COSTS_PATH=
CLAWOS_OBSERVABILITY_DIR=
CLAWOS_OBSERVABILITY_PATH=
CLAWOS_VOICE_OUTPUT_DIR=

# Voice providers
STT_PROVIDER=mock
TTS_PROVIDER=mock

# Required only when STT_PROVIDER=openai
OPENAI_API_KEY=
OPENAI_STT_MODEL=whisper-1
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Required only when TTS_PROVIDER=elevenlabs
ELEVENLABS_API_KEY=
ELEVENLABS_DEFAULT_VOICE_ID=
ELEVENLABS_MODEL_ID=eleven_multilingual_v2

# Required only when CLAWOS_EMBEDDING_PROVIDER=google
GOOGLE_API_KEY=
GOOGLE_EMBEDDING_MODEL=gemini-embedding-001

# Security controls
API_RATE_LIMIT_MAX=60
API_RATE_LIMIT_WINDOW_MS=60000
CLAWOS_REQUIRE_GLOBAL_MEMORY_APPROVAL=false

# Remote access checklist inputs
OPENCLAW_WS_URL=ws://127.0.0.1:18789
REMOTE_ACCESS_PROVIDER=none
TAILSCALE_FUNNEL_URL=
CLOUDFLARE_TUNNEL_URL=
